<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.store59.kylin.location.data.mapper.ProvinceMapper">
	<resultMap id="BaseResultMap" type="com.store59.kylin.location.data.model.Province">
		<id column="province_id" property="provinceId" jdbcType="INTEGER" />
		<result column="name" property="name" jdbcType="VARCHAR" />
	</resultMap>
	<sql id="Base_Column_List">
		province_id,name
	</sql>
	<select id="getProvinceList" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from 59_province
	</select>
</mapper>